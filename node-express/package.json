{
  "name": "rss-feed-summarization-platform",
  "version": "1.0.0",
  "description": "Professional multi-user RSS/Atom feed summarization platform using OpenAI GPT models",
  "main": "backend/app.js",
  "type": "module",
  "scripts": {
    "start": "node backend/app.js",
    "dev": "nodemon backend/app.js",
    "build": "npm install && cd frontend && npm run build",
    "test": "jest",
    "frontend:dev": "cd frontend && npm run dev",
    "frontend:build": "cd frontend && npm run build",
    "frontend:install": "cd frontend && npm install",
    "dev:full": "concurrently \"npm run dev\" \"npm run frontend:dev\"",
    "feeds:fetch-all": "node scripts/manage-feeds.js fetch-all",
    "feeds:recent": "node scripts/manage-feeds.js recent-articles",
    "feeds:count": "node scripts/manage-feeds.js count-articles",
    "feeds:help": "node scripts/manage-feeds.js help",
    "migrate:duplicates": "node backend/scripts/migrate-duplicate-prevention.js",
    "migrate:duplicates:clean": "node backend/scripts/migrate-duplicate-prevention.js --clean-duplicates"
  },
  "keywords": [
    "rss",
    "atom",
    "feed",
    "summarization",
    "gpt",
    "nodejs",
    "express",
    "vue"
  ],
  "author": "RSS Summarization Team",
  "license": "MIT",
  "dependencies": {
    "axios": "^1.6.2",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "express-validator": "^7.0.1",
    "feedparser": "^2.2.10",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.6.5",
    "node-cron": "^3.0.3",
    "openai": "^4.20.1",
    "rss-parser": "^3.13.0",
    "sequelize": "^6.35.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.6",
    "babel-jest": "^29.7.0",
    "concurrently": "^9.2.1",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  }
}
